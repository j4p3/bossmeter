{
  "name": "moodmeter-api",
  "version": "0.1.0",
  "private": true,
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"npm run start\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "JP Bonner <bonnerjp@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "debug": "^2.6.1",
    "express": "^4.13.3",
    "graphql": "^0.8.2",
    "mongoose": "^4.7.7",
    "request-promise": "^4.1.1",
    "watson-developer-cloud": "^2.15.2"
  },
  "devDependencies": {
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  },
  "homepage": ""
}
